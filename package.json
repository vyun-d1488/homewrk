	"license": "MIT",
	"scripts": {
		"server:start": "node ./build/app.js",
		"server:dev": "nodemon --exec babel-node ./src/index.js",
		"build": "concurrently \"yarn build:dev\" \"yarn build:babel\"",
		"build:clean": "rimraf ./build",
		"build:babel": "babel -w src --out-dir build/ --copy-files",
		"build:dev": "cross-env NODE_ENV=development webpack --config internals/webpack.client.js",
		"build:prod": "cross-env NODE_ENV=production webpack --config internals/webpack.client.js"
	},
	"devDependencies": {
		"@babel/cli": "^7.10.5",
		"@babel/core": "^7.10.5",
		"@babel/node": "^7.10.5",
		"@babel/plugin-proposal-class-properties": "^7.10.4",
		"@babel/plugin-transform-runtime": "^7.11.0",
		"@babel/preset-env": "^7.10.4",
		"@babel/preset-react": "^7.10.4",
		"babel-plugin-root-import": "^6.5.0"
	},
	"dependencies": {
		"@material-ui/core": "^4.11.0",
		"axios": "^0.19.2",
		"babel-loader": "^8.1.0",
		"copy-webpack-plugin": "^6.1.0",
		"cross-env": "^7.0.2",
		"css-loader": "^3.6.0",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"extract-text-webpack-plugin": "^3.0.2",
		"html-webpack-inline-chunk-plugin": "^1.1.1",
		"html-webpack-plugin": "^4.4.1",
		"mini-css-extract-plugin": "^0.9.0",
		"nodemon": "^2.0.4",
		"progress-bar-webpack-plugin": "^2.1.0",
		"react": "^16.13.1",
		"react-custom-scrollbars": "^4.2.1",
		"react-dom": "^16.13.1",
		"react-parallax-scroll": "^0.1.1",
		"react-scroll-parallax": "^2.3.4",
		"react-scrollmagic": "^2.2.0",
		"react-spring": "^8.0.27",
		"react-use": "^15.3.3",
		"rimraf": "^3.0.2",
		"terser-webpack-plugin": "^4.1.0",
		"webpack": "^5.0.0-beta.29",
		"webpack-cli": "^3.3.12"
	}
